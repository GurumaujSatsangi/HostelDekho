<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.13.1/font/bootstrap-icons.min.css">
  </head>
  <body>
    <%- include('partials/navbar') %> <% if(hosteldata && hosteldata.length>0){
    %> <% hosteldata.forEach(function(hostels){ %>

    <div class="card">
      <div class="card-body">
        <h5 class="card-title"><%= hostels.hostel_name %></h5>
        <a href="/view/hostel/<%= hostels.hostel_id %>" class="btn btn-primary"
          >Explore <i class="bi bi-arrow-right-circle-fill"></i></a
        >
      </div>
    </div>

    <% }) %> <% } %> <%- include('partials/footer') %>
  </body>
</html>
